class Solution {
public:
    int search(vector<int>& nums, int target) {
        if(find(nums.begin(),nums.end(),target)==nums.end()){
            return -1;
        }
        else{
            int i=0;
            while(nums[i]!=target)
            {
                i++;
            }
            return i;
            
        }
    }
};
